    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug on device",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/glmark2-es2-wayland",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Включить автоматическое форматирование для gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "Deploy nemo",
            // here you should paste your SDK path
            "miDebuggerPath": "${workspaceFolder}/../../SailfishOS/bin/gdb-armv7hl-meego-linux-gnueabi",
            // "miDebuggerServerAddress": "192.168.2.15:10001",  // default device IP, port is setuped in tasks.json
            "miDebuggerServerAddress": "192.168.2.19:10001",  // default device IP, port is setuped in tasks.json
        },
    ]
}